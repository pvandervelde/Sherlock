<?xml version="1.0" encoding="utf-8" ?>
<sherlockreport version="${REPORT_VERSION}$">
    <header>
        <sherlock>
            <host>${HOST_NAME}$</host>
            <user>${USER_NAME}$</user>
            <description>${DESCRIPTION}$</description>
            <version>${SHERLOCK_VERSION}$</version>
            <starttime>${TEST_START_TIME}$</starttime>
            <endtime>${TEST_END_TIME}$</endtime>
        </sherlock>
        <product>
            <name>${PRODUCT_NAME}$</name>
            <version>${PRODUCT_VERSION}$</version>
        </product>
        <result>
            ${TEST_RESULT}$
        </result>
    </header>
    <tests>
        ${TEST_SECTIONS}$
    </tests>
</sherlockreport>